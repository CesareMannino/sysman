{{!-- First name --}}
  <div class="row">
    <div class="col-md-2">
      <input type="text" class="form-control" id="floatingInput" value="{{this.first_name}}" placeholder="First Name" name="first_name">
    </div>
{{!-- Last name --}}
    <div class="col-md-2">
      <input type="text" class="form-control" 
      id="floatingInput" value="{{this.last_name}}"  placeholder="Last Name" name="last_name">
    </div>

{{!-- drop down menu stcw coc --}}
    <div class="col-md-3">
      <select class="form-select" id="inputGroupSelect01" value="{{this.coc}}" name='coc'>
        <option selected>Certificate of Competency/Proficiency</option>
        <option value="Master DP II/2 IV/2">Master DP II/2 IV/2</option>
        <option value="Master II/2 IV/2">Master II/2 IV/2</option>
        <option value="Chief Mate DP II/2 IV/2">Chief Mate DP II/2 IV/2</option>
        <option value="Chief Mate II/2 IV/2">Chief Mate II/2 IV/2</option>
        <option value="OOW Navigational Watch DP II1/IV2">OOW Navigational Watch DP II1/IV2</option>
        <option value="OOW Navigational Watch II1/IV2">OOW Navigational Watch II1/IV2</option>
        <option value="Deck Cadet">Deck Cadet</option>
        <option value="Radio operator IV/2">Radio operator IV/2</option>
      </select>




      {{!-- date picker coc--}}

    </div>

    <div class="col-md-3">

      <input type="text" id="datepicker" value="{{this.expiration}}" placeholder="exp. date (mm/dd/yy)" name='expiration'>
    </div>
  


{{!-- phone number --}}
  <div class="row">

    <div class="col-md-2">
      <br>
      <input type="text" class="form-control" value="{{this.phone}}" placeholder="Phone" name="phone">

    </div>

    <div class="col-md-2">
      <br>
      <input type="email" class="form-control" value="{{this.email}}" placeholder="email@email.com" name="email">
    </div>
  </div>

  {{!-- PSSR --}}
  <div class="row">

    <div class="col-md-4">
      <br>
      <input class="form-control" type="text" placeholder="PSSR A-VI/1 - 1" aria-label="readonly input example"
        readonly>

    </div>

    <div class="col-md-3">
      <br>
      <input type="text" id="datepickerPSSR" value="{{this.PSSR}}" placeholder="exp. date (mm/dd/yy)" name="PSSR">

    </div>
  </div>


  <style>
    #datepicker,
    #datepickerPSSR,
    #datepickerFFB,
    #datepickerADV {
      padding: 5.5px;
      border-radius: 5px;
      border: 1.2px solid rgb(184, 184, 184);
      outline:
        outline-color: blue;
    }
  </style>


  <div class="row">

    <div class="col-md-4">
      <br>
      <input class="form-control" type="text" placeholder="FIRE PREVENTION AND FIREFIGHTING (STCW A-VI/1-2)"
        aria-label="readonly input example" readonly>

    </div>

    <div class="col-md-3">
      <br>
      <input type="text" id="datepickerFFB" value="{{this.FFB}}" placeholder="exp. date (mm/dd/yy)" name='FFB'>
    </div>
  </div>


  <div class="row">

    <div class="col-md-4">
      <br>

      <input class="form-control" type="text" placeholder="ADVANCED FIRE FIGHTING STCW A-VI/3"
        aria-label="readonly input example" readonly>
    </div>

    <div class="col-md-3">
      <br>
      <input type="text" id="datepickerADV" value="{{this.ADV}}" placeholder="exp. date (mm/dd/yy)" name='ADV'>
    </div>
  </div>


  <style>
    #datepicker,
    #datepickerPSSR,
    #datepickerADV {
      padding: 5.5px;
      border-radius: 5px;
      border: 1.2px solid rgb(184, 184, 184);
      outline:
        outline-color: blue;
    }
  </style>


  <div class="col-1 d-grid">
    <button class="btn btn-primary" type="submit" style="margin:90px 0 0 0">Submit</button>
