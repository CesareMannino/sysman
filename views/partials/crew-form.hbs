{{!-- First name --}}
<div class="row">
  <div class="col-md-2">
    <input type="text" class="form-control" id="floatingInput" value="{{this.first_name}}" placeholder="First Name"
      name="first_name">
  </div>
  {{!-- Last name --}}
  <div class="col-md-2">
    <input type="text" class="form-control" id="floatingInput" value="{{this.last_name}}" placeholder="Last Name"
      name="last_name">
  </div>

  {{!-- drop down menu stcw coc --}}
  <div class="col-md-3">
    <select class="form-select" id="inputGroupSelect01" value="{{this.coc}}" name='coc'>
      <option selected>Certificate of Competency/Proficiency</option>
      <option value="Master DP II/2 IV/2">Master DP II/2 IV/2</option>
      <option value="Master II/2 IV/2">Master II/2 IV/2</option>
      <option value="Chief Mate DP II/2 IV/2">Chief Mate DP II/2 IV/2</option>
      <option value="Chief Mate II/2 IV/2">Chief Mate II/2 IV/2</option>
      <option value="OOW Navigational Watch DP II1/IV2">OOW Navigational Watch DP II1/IV2</option>
      <option value="OOW Navigational Watch II1/IV2">OOW Navigational Watch II1/IV2</option>
      <option value="Deck Cadet">Deck Cadet</option>
      <option value="Radio operator IV/2">Radio operator IV/2</option>
    </select>




    {{!-- date picker coc--}}

  </div>

  <div class="col-md-3">

    <input type="text" id="datepicker" value="{{this.expiration}}" placeholder="exp. date (mm/dd/yy)" name='expiration'>
  </div>



  {{!-- phone number --}}
  <div class="row">

    <div class="col-md-2 mb-5">
      <br>
      <input type="text" class="form-control" value="{{this.phone}}" placeholder="Phone" name="phone">

    </div>
{{!-- email --}}
    <div class="col-md-3 mb-5">
      <br>
      <input type="email" class="form-control" value="{{this.email}}" placeholder="email@email.com" name="email">
    </div>
  </div>
